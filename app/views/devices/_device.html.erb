<tr id="device-<%= device.id %>">
	<td>
		<%= link_to device.device_name, device_path(device) %>
	</td>
	<td>
		<%= device.type %>
	</td>
	<td>
		<%= device.os_version %>
	</td>
	<td>
		<%= device.office_version %>
	</td>
	<td>
		<%= device.current_email %>
	</td>
	<td>
		<%= device.current_browser %>
	</td>
	<td>
		<%= device.new_email %>
	</td>
</tr>